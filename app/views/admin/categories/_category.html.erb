<li>
  <div class="col-md-2">
    <%= link_to category.name, admin_category_path(category) %>
  </div>
  <div class="col-md-4">
    <%= t ".created_at" %> : <%= l category.created_at, format: :long %>
  </div>
  <div class="col-md-2">
    <%= link_to t(".delete"), admin_category_path(category),
      class: "btn btn-xs btn-danger",method: :delete,
      data: {confirm: t(".ask_before_delete")} %>
  </div>
  <div class="col-md-2">
    <%= link_to t(".update"), edit_admin_category_path(category),
      class: "btn btn-xs btn-warning" %>
  </div>
</li>
